{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "baseUrl": "./",
    "importHelpers": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "typeRoots": [
      "node_modules/@types",
    ],
    "types": [
      "node",
      "jest",
      "reflect-metadata",
      "./src/Types/External/index"
    ],
    "esModuleInterop": true,
    "inlineSourceMap": true
  }
}
